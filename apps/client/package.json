    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@kyvg/vue3-notification": "^3.4.1",
    "axios": "^1.11.0",
    "core-js": "^3.8.3",
    "pinia": "^3.0.3",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.4.0",
    "vue3-smooth-scroll": "^0.8.1",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
